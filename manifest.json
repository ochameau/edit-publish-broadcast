{

  "manifest_version": 2,
  "name": "Edit-Publish-Broadcast",
  "version": "1.0",

  "description": "Make the web yours and editable.",

  "icons": {
    "48": "icons/edit.svg"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["epb.js"]
    }
  ],

  "permissions": ["http://*/*", "https://*/*"]
}
